{
  "name": "daosys",
  "version": "0.0.1-alpha",
  "description": "Daosys site, smart contracts, and documentation.",
  "main": "index.js",
  "repository": "git@github.com:syscoin/daosys.git",
  "author": "cyotee doge <cyotee@syscoin.org>",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "clean": "yarn run hardhat clean",
    "compile": "yarn run hardhat compile && yarn run hardhat typechain",
    "typechain": "yarn run hardhat typechain",
    "test": "yarn run hardhat test",
    "coverage": "yarn run hardhat coverage",
    "storage-layout": "yarn run hardhat storage-layout",
    "build": "yarn run hardhat clean && yarn run hardhat compile && yarn run hardhat typechain && yarn run hardhat test --logs && lerna run tsc",
    "clean-build": "yarn run hardhat clean && yarn run hardhat compile && yarn run hardhat test --logs",
    "full-build": "yarn run hardhat clean && yarn run hardhat compile && yarn run hardhat typechain && yarn run hardhat test --logs && yarn docs",
    "docs": "asciidoctor-reducer --preserve-conditionals -o docs/daosys-documentation.adoc docs/daosys-documentation-template.adoc",
    "litepaper": "asciidoctor-reducer --preserve-conditionals -o docs/daosys-litepaper.adoc docs/daosys-litepaper-template.adoc",
    "full-docs": "yarn docs && yarn litepaper"
  },
  "devDependencies": {
    "@asciidoctor/core": "^2.2.6",
    "@ethersproject/abi": "^5.6.3",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/providers": "^5.6.8",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.0.4",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "@openzeppelin/hardhat-upgrades": "^1.18.1",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.39",
    "asciidoctor": "^2.2.6",
    "asciidoctor-pdf": "^1.0.0-alpha.14",
    "chai": "^4.3.6",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.6.8",
    "hardhat": "^2.9.2",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "hardhat-storage-layout": "^0.1.6",
    "hardhat-tracer": "^1.1.0-rc.6",
    "lerna": "^5.1.1",
    "solidity-coverage": "^0.7.21",
    "stochasm": "^0.5.0",
    "ts-node": "^10.8.1",
    "typechain": "^8.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {}
}
